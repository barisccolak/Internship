/JOB
//NAME TEST_XIRIS_TRIGGER
///FOLDERNAME TEST_JOBS
//POS
///NPOS 0,0,0,0,0,0
//ALIAS
///LVARS 0,0,1,0,0,0,0,0
LD000 XIRIS_NR
//ARGINFO
///ARGTYPE D,,,,,,,
///COMMENT
XIRIS_NR







//INST
///DATE 2023/09/25 13:51
///ATTR SC,RW
///GROUP1 RB1
///LVARS 0,0,1,0,0,0,0,0
NOP
'--------------------------------
'Dieser Unterjob triggert ein
'einzelnes Xiris-Video.
'Es werden keine IDs gesetzt oder
'verändert.
'Den Job nicht als Hauptjob
'starten, nur mit CALL aufrufen.
'--------------------------------
'Kamera auswählen
GETARG XIRIS_NR IARG#(1)
CALL JOB:TRIGGER ARGF"UI_START"
TIMER T=1.00
CALL JOB:XIRIS ARGFXIRIS_NR ARGF"VIDEO_EIN"
TIMER T=0.50
CALL JOB:TRIGGER ARGF"SCHWEISSEN_EIN"
TIMER T=1.00
CALL JOB:XIRIS ARGFXIRIS_NR ARGF"TRIG_EIN"
TIMER T=10.00
CALL JOB:XIRIS ARGFXIRIS_NR ARGF"TRIG_AUS"
TIMER T=0.50
CALL JOB:TRIGGER ARGF"SCHWEISSEN_AUS"
TIMER T=0.50
CALL JOB:XIRIS ARGFXIRIS_NR ARGF"VIDEO_AUS"
TIMER T=1.00
CALL JOB:TRIGGER ARGF"UI_STOP"
TIMER T=1.00
CALL JOB:TRIGGER_RESET
END
